# Indian Stock Exchange MCP Server Configuration
# Copy this file to .env and fill in your actual values

# API Key for Indian Stock Exchange API (REQUIRED)
ISE_API_KEY=your_api_key_here

# API Base URL
ISE_API_BASE_URL=https://stock.indianapi.in/

# Server Configuration
ISE_HTTP_HOST=0.0.0.0
ISE_HTTP_PORT=8000

# Request timeout in seconds
ISE_REQUEST_TIMEOUT=30

# Logging level (DEBUG, INFO, WARNING, ERROR)
ISE_LOG_LEVEL=INFO
